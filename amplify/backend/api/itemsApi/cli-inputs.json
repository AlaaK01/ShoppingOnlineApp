{
  "version": 1,
  "paths": {
    "/products": {
      "name": "/products",
      "lambdaFunction": "itemsLambda",
      "permissions": {
        "setting": "open"
      }
    },
    "/users": {
      "name": "/users",
      "lambdaFunction": "itemsLambda",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    }
  }
}